            <simplebutton name="btnApplyLoadedPrefab" depth="3" caption_key="xuiPrefabsApplyLoadedPrefab" tooltip_key="xuiPrefabsApplyLoadedPrefabTooltip" width="93" height="30" font_size="30" />
